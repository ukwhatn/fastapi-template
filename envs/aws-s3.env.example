# S3互換ストレージの場合
S3_ENDPOINT=http://your-storage-endpoint:port
S3_ACCESS_KEY=your_access_key
S3_SECRET_KEY=your_secret_key

# AWS S3の場合
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
# AWS_REGION=ap-northeast-1

# 共通設定
S3_BUCKET=your-backup-bucket
BACKUP_DIR=default
BACKUP_RETENTION_DAYS=30
BACKUP_TIME=03:00
# 時間を個別に指定する場合（BACKUP_TIMEより優先）
# BACKUP_HOUR=3
# BACKUP_MINUTE=0

# 実行モード: scheduled (定期実行) または interactive (対話モード)
DUMPER_MODE=scheduled